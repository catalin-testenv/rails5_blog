{
  "name": "rails_blog",
  "version": "0.0.1",
  "description": "rails blog",
  "author": "catalinenache78@gmail.com",
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.10.1",

    "react": "^0.14.7",
    "react-dom": "^0.14.7",

    "flux": "2.1.1",

    "react-router": "2.0.0",

    "keymirror": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "uglify-js": "^2.6.1",

    "babel-jest": "6.0.1",
    "jest-cli": "0.8.2",
    "react-addons-test-utils": "~0.14.5"
  },

  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "moduleFileExtensions": ["js", "jsx"],
    "testFileExtensions": ["js", "jsx"]
  },

  "browserify": {
    "transform": [
      "babelify"
    ]
  },

  "scripts": {
    "setup": "npm install && bundle install",
    "js": "browserify --extension=.jsx -o app/assets/javascripts/admin.js react_admin/admin.jsx",
    "experiment": "browserify -o ./_experiment.js ./experiment.js && node ./_experiment.js && rm -f ./_experiment.js",
    "test": "jest"
  }
}